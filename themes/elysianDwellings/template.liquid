              <div class="home-stats-number">{{item.value}}</div>
              <div class="home-stats-text">{{item.label}}</div>
            </div>
          </div>
      {% endfor %}
    </div>
  </div>
</section>

<section class="home-featureProperties">
  <div class="home-featureProperties__heading">{{ feature_properties.title }}</div>
  <div class="home-featureProperties__container" id="propertySlider">
    {% for property in feature_properties.list %}
    <div class="home-featureProperties__card">
      <img src="{{assetsUrl}}{{ property.image }}" alt="{{ property.name }}" class="home-featureProperties__image" />
      <div class="home-featureProperties__tags">
        <div class="home-featureProperties__tag">{{ property.type }}</div>
        <div class="home-featureProperties__tag home-featureProperties__tag--units">
          <span>Available Units :</span>
          <span class="home-featureProperties__units">{{ property.units }}</span>
        </div>
      </div>
      <div class="home-featureProperties__info">
        <h3 class="home-featureProperties__title">{{ property.name }}</h3>
        <div class="home-featureProperties__details">
          {% for detail in property.details %}
          <div class="home-featureProperties__detail">
            <div class="home-featureProperties__detail-value">{{ detail.value }}</div>
            <div class="home-featureProperties__detail-label">{{ detail.label }}</div>
          </div>
          {% unless forloop.last %}
          <svg xmlns="http://www.w3.org/2000/svg" width="2" height="71" viewBox="0 0 2 71" fill="none">
            <path d="M1 0V71" stroke="#696969" />
          </svg>
          {% endunless %}
          {% endfor %}
        </div>
        <div class="home-featureProperties__price-container">
          <div class="home-featureProperties__price">
            <span>Price :</span>
            <div class="home-featureProperties__price-value">
              {{ property.price_icon | raw }}
              <span>{{ property.price }}</span>
            </div>
          </div>
          <a href="{{ property.link }}" class="home-featureProperties__btn">View Details</a>
        </div>
      </div>
    </div>
    {% endfor %}
  </div>
</section>

